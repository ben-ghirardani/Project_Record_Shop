<h1>Albums</h1>

<a href="/stock">Back to Stock</a>

<table>
    
    <tr>
      <th>Album Name</th>
      <th></th>
      <th>Genre</th>
      <th></th>
    </tr>
    <% for album in @albums %>
    <tr>
      <td><%= album.name %></td>
      <td>|</td>
      <td><%= album.genre %></td>
      <td>|</td>   
      <td><a href="/albums/<%= album.id %>/delete"> Delete Album? </a></td>
    </tr>
    <% end %>


</table>